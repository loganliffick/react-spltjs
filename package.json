    "dist"
  ],
  "type": "module",
  "main": "./dist/react-splt.js",
  "types": "./dist/react-splt.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-splt.js",
      "require": "./dist/react-splt.umd.cjs"
    }
  },
  "version": "0.0.2",
  "description": "SpltJs for React",
  "author": "Logan Liffick",
  "license": "MIT",
  "repository": "loganliffick/react-spltjs",
  "keywords": [
    "react",
